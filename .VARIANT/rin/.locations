#List of config -> system file relations for rin

System/.xinitrc               > $HOME/.xinitrc
System/i3/config              > $HOME/.config/i3/config
System/.bash_profile          > $HOME/.bash_profile
System/.bashrc                > $HOME/.bashrc
System/fonts                  > $HOME/.fonts
System/environment            > /etc/environment
System/logind.conf            > /etc/systemd/logind.conf
System/alsa-base.conf         > /etc/modprobe.d/alsa-base.conf
System/vconsole.conf          > /etc/vconsole.conf
System/systemd/coredump.conf  > /etc/systemd/coredump.conf
System/env_config             > $HOME/.config/env_config

System/.Xdefaults             > $HOME/.Xdefaults
System/.Xresources            > $HOME/.Xresources
System/Xorg/*                 > /etc/X11/xorg.conf.d/

System/torrc                  > /etc/tor/torrc

System/ssh                    > /etc/ssh
System/ssh/known_hosts        > $HOME/.ssh/known_hosts
System/ssh/sshd.socket        > /etc/systemd/system/sshd.socket

System/service/*              > /etc/systemd/system/

LAMP/httpd                    > /etc/httpd
LAMP/http                     > /srv/http
LAMP/php                      > /etc/php
LAMP/phpmyadmin               > /etc/webapps/phpmyadmin

vim/vimrc                     > /etc/vimrc
vim/colors                    > $HOME/.vim/colors
vim/colors                    > /root/.vim/colors

MPD/mpv                       > $HOME/.config/mpv

rofi/default.rasi             > $HOME/.local/share/rofi/themes/default.rasi

Apps/windows_kvm.sh           > /usr/local/bin/win_kvm
scripts/pidlock.sh            > /usr/local/bin/pidlock

Themes/Flat-Remix/Flat Remix  > /usr/share/icons/Flat_Remix

eagle                         > $HOME/eagle
Trolltech.conf                > $HOME/.config/Trolltech.conf
ranger                        > $HOME/.config/ranger
